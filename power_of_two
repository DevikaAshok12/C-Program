/*Program to print the power of two series using for loop*/
#include <stdio.h>
int main()
{
    int num;
    int power=1;
    printf("Enter the number :");
    scanf("%d",&num);
    if(num>0)
    {
        for(int i=0;i<num;i++)
        {
            printf("%d ",power);
            power=power*2;
            
        }
    }
    else
    {
        printf("Error:Number should be an positive number.");
    }
    return 0;
}
