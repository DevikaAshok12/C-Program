#include <stdio.h>
void reverse_array(int array[],int size)
{
    for(int i=0;i<size;i++)
    {
        array[i]=array[size-i-1];
    }
}

int main()
{
    int size;
    printf("Enter array size");
    scanf("%d",&size);
    printf("Enter array elements");
    int array[size];
    for(int i=0;i<size;i++)
    {
        scanf("%d",&array[i]);
    }
    reverse_array(array,size);
    for(int i=0;i<size;i++)
    {
        printf("%d index element is %d\n",i,array[i]);
    }
    return 0;

}
