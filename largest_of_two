/*You have to read two integers from user and find the maximum of two integers.*/
#include<stdio.h>
int main()
{
    int num1,num2;
    printf("Enter the num1:");
    scanf("%d",&num1);
    printf("Enter the num2:");
    scanf("%d",&num2);
    if(num1>num2)
    {
        printf("Max of two numbers is %d",num1);
    }
    else
    {
        printf("Max of two numbers is %d",num2);
    }
    return 0;
}
