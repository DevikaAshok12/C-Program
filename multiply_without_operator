/*Program to multiply 2 nos without multiplication operator*/
#include <stdio.h>
int main()
{
    int n1,n2;
    int result=0;
    printf("Enter the two numbers :");
    scanf("%d %d",&n1,&n2);
    for(int i=1;i<=n2;i++)
    {
        result=result+n1;
        
    }
    printf("The Result is %d",result);
    return 0;
}
