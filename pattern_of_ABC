#include <stdio.h>

int main()
{
    int range;
    char alpha = 65;
    printf("Enter the range ");
    scanf("%d",&range);
    for(int i=1;i<=range;i++)
    {
        for(int j=1;j<=i;j++)
        {
            printf("%c ",alpha);
            alpha++;
        }
        printf("\n");
    }
return 0;


}
