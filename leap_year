#include <stdio.h>
int main()
{
   int year;
   printf("Enter the year");
   scanf("%d",&year);
   if ( year%100 == 0)
   {  
       if(year%400 == 0)
       {
          printf("$year is a century leap year");
       }
       else
       {
          printf("year is not a century leap year");
       }
  }
  else if (year%4 == 0 )
  {
      printf("its a leap yr");
  }
  else
  {
     printf("year is not a leap year");
  }
return 0;
}

